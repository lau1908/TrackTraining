@model TrackTraining.Models.LeaderboardData




@{
    ViewBag.Title = "About";
    
}

<h2>Leaderboards</h2>

    @foreach (var item in Model.Øvelser) //kører igennem dataen med øvelserne
    {
        var kk = item.OvelseId;//sætter variablen til at vær lig, øvelsesId'et
        
        <h1>@item.OvelseNavn</h1> //sætter navnet ind
        var data = Model.data.Where(x => x.Ovelser.OvelseId == kk).Take(5); //laver en liste med data der matcher id'et på den øvelse, og tager de første 5 datasæt

        <div class="table-wrapper"> @*//laver et table*@
            <table class="fl-table">
                <tr>
                    @*<th>Plads</th>*@
                    <th>Bruger</th>
                    @*<th>Øvelse</th>*@
                    <th>Gentagelser</th>
                    <th>Dato</th>

                </tr>
                
                
                    @foreach (var item1 in data) //kører igennem dataen, og indsætter den
                        
                        {
                            
                        <tr class="Leadreko">


                            @*<td id="number"></td>*@
                            <td>@item1.AspNetUser.UserName</td> 
                            @*<td>@item1.Ovelser.OvelseNavn</td>*@
                            <td>@item1.Gentagelser</td>
                            <td>@item1.dato.ToShortDateString()</td>



                        </tr>






                    }
                



            </table>
        </div>
    }


