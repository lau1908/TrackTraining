@model IEnumerable<TrackTraining.DBModels.Ovelser>
<div class="Ovelsewrapper">

    @foreach (var o in Model) //kører igennem dataen, og indsætter den
    {
    <div>
        <div class="" id="kk">

            <div class="kkkk" id="ol">
                <a href="@Url.Action(
                     "Øvelser", "Home", new RouteValueDictionary(new { ovelseId = o.OvelseId }) //hvis der trykkes på denne, ville den sende et øvelsesid med videre til siden Øvelser
                         )">
                    <img id="umg" src=@o.Billede width="200" class="bild" />

                </a>


            </div>

            <div class="descr ">
                <a href="@Url.Action(
                     "Øvelser", "Home", new RouteValueDictionary(new { ovelseId = o.OvelseId })//hvis der trykkes på denne, ville den sende et øvelsesid med videre til siden Øvelser
                         )">
                    <span class="navn"> @o.OvelseNavn</span>

                </a>

            </div>
        </div>
    </div>
    }

        

</div>